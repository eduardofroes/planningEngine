propose findRedJewel() {
    fetch input where jewel.color == "red" in i1

    if (i1 != null) {
        return applyFindJewelRed(i1)
    }else{
        return null
    }
}

apply goToRedJewel(Identifier i1) {
    Identifier operator
    operator.name = "goToRedJewel"
    operator.jewel = i1

    save operator in memory.operators
    delete i1   
}